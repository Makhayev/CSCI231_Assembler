.data

	
	fib: .word 4
	

.text
	main:
		lw $a1, fib
		jal findfib
		addi $a0, $v0, 0
		
		li $v0, 1
		syscall

		li $v0, 10
		syscall

	findfib:
		addi $sp, $sp, -8
		sw $a1, ($sp)
		sw $ra, 4($sp)
		slti $t2, $a1, 1
		beq $t0, $zero, loop
		addi $sp, $sp, 8
		
		jr $ra
		
		
	loop:	addi $a1, $a1, -1
		jal findfib
		lw $a1, ($sp)
		lw $ra, 4($sp)
		addi $sp, $sp, 8
		mul $v0, $v0, $a1
		jr $ra
		
		
		
		
		
	
	
	



	
	
	
	
	
	